function post_error(msg,param,holder){var m='';var p=undefined;var h=$('body');var jn=false;if(msg.responseJSON!=undefined){m=msg.responseJSON.msg;jn=true;}
else if(msg.reponseText!=undefined){m=msg.responseText;}
else{m=msg;}
if(jn){p=msg.responseJSON.param;}
else{p=param;}
if(holder!=undefined&&holder!=null){if(holder.length){h=holder;}}
else{holder=$('body');}
show_error(m);if(p!=undefined&&p!=null){var x=h.find('[name='+p+']');x.parents().find('div.collapse').has(x).collapse('show');if(x.hasClass('select2-hidden-accessible')){x=x.next().find('.select2-selection--single');}
x.focus();x.addClass('elem-focus');setTimeout(function(){holder.find('[name='+p+']').removeClass('elem-focus');},1500)}}
