Date.GregorianToPersian=function(g_y,g_m,g_d)
{g_y=parseInt(g_y);g_m=parseInt(g_m);g_d=parseInt(g_d);var gy=g_y-1600;var gm=g_m-1;var gd=g_d-1;var g_day_no=365*gy+parseInt((gy+3)/4)-parseInt((gy+99)/100)+parseInt((gy+399)/400);for(var i=0;i<gm;++i)
