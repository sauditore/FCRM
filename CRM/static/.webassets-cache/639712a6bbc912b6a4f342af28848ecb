Date.isPValid=function(y,m,d,h,i,s,ms){h=h||0;i=i||0;s=s||0;ms=ms||0;if(!Date.PcheckDate(y,m,d))return false;g=Date.PersianToGregorian(y,m,d);y=g[0];m=g[1];d=g[2];var dt=new Date(y,m-1,d,h,i,s,ms);return y==dt.getFullYear()&&m==dt.getMonth()+1&&d==dt.getDate()&&h==dt.getHours()&&i==dt.getMinutes()&&s==dt.getSeconds()&&ms==dt.getMilliseconds();}
Date.PparseRegexes=[];Date.PparseDate=function(input,format,strict){var p=Date.PparseFunctions;if(p[format]==null){Date.PcreateParser(format);}
