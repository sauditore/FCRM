Date.prototype.getWeekOfYear=function(){var days=this.getPDayOfYear();return Math.ceil(days/7);}
Date.prototype.isKabiseYear=function(){var year=this.getPFullYear();var mod=year%33;if(mod==1||mod==5||mod==9||mod==13||mod==17||mod==22||mod==26||mod==30)return true;return false}
