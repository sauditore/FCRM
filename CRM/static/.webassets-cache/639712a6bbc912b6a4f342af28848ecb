Date.prototype.settPUTCMonth=function(m,d){var gd=this.getUTCDate();var gm=this.getUTCMonth();var gy=this.getUTCFullYear();var j=Date.GregorianToPersian(gy,gm+1,gd);if(m>11){j[0]+=math.floor(m/12);m=m%12;}
